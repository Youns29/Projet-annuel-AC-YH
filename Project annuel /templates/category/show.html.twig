{% extends 'base.html.twig' %}

{% block title %}
  Author
{% endblock %}

{% block body %}
  <h1>
    Author
  </h1>

  <table class="table">
    <tbody>
      <tr>
        <th>
          Id
        </th>
        <td>
          {{ Author.id }}
        </td>
      </tr>
      <tr>
        <th>
          Name
        </th>
        <td>
          {{ Author.name }}
        </td>
      </tr>
    </tbody>
  </table>

  <a href="{{ path('app_Author_index') }}">back to list</a>

  <a href="{{
    path(
      'app_Author_edit',
      {
        id: Author.id
      }
    )
    }}">
    edit
  </a>

  {{ include('Author/_delete_form.html.twig') }}
{% endblock %}
